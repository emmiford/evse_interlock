#include <zephyr/dt-bindings/gpio/gpio.h>

/ {
	chosen {
		zephyr,lora-transceiver = &lora;
	};

	aliases {
		lora-transceiver = &lora;
		extinput0 = &extinput0;
	};

	gpio_keys: gpio_keys {
		compatible = "gpio-keys";
		extinput0: extinput0 {
			gpios = <&gpio0 11 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>;
		};
	};
};

arduino_spi: &spi1 {
};

lora_semtech_sx126xmb2xxs: &lora {
};
